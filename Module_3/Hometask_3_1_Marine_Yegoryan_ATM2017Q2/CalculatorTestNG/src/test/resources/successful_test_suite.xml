<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Calculator" verbose="1" parallel="true" thread-count="2">

	<test name="MavenTest">
		<groups>
			<run>
				<exclude name="failed" />
			</run>
		</groups>
		<classes>
			<class name="testng_tests.CosTest" />
			<class name="testng_tests.CtgTest" />
			<class name="testng_tests.CtgTestException" />
			<class name="testng_tests.DivDoubleTest" />
			<class name="testng_tests.DivDoubleTestException" />
			<class name="testng_tests.DivLongTest" />
			<class name="testng_tests.DivLongTestException" />
			<class name="testng_tests.IsNegativeTest" />
			<class name="testng_tests.IsPositiveTest" />
			<class name="testng_tests.MultiplyDoubleTest" />
			<class name="testng_tests.MultiplyTest" />
			<class name="testng_tests.PowTest" />
			<class name="testng_tests.SinTest" />
			<class name="testng_tests.SubDoubleTest" />
			<class name="testng_tests.SubLongTest" />
			<class name="testng_tests.SumDoubleTest" />
			<class name="testng_tests.SumLongTest" />
			<class name="testng_tests.TgTest" />
			<class name="testng_tests.TgTestException" />
		</classes>
	</test>
</suite> 
