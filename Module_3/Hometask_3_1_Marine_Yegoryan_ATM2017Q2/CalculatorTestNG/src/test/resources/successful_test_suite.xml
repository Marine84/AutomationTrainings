<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Calculator" verbose="1" parallel="true" thread-count="2">

	<test name="MavenTest">
		<groups>
			<run>
				<exclude name="failed" />
			</run>
		</groups>
		<classes>
			<class name="MavenTest.CosTest" />
			<class name="MavenTest.CtgTest" />
			<class name="MavenTest.CtgTestException" />
			<class name="MavenTest.DivDoubleTest" />
			<class name="MavenTest.DivDoubleTestException" />
			<class name="MavenTest.DivLongTest" />
			<class name="MavenTest.DivLongTestException" />
			<class name="MavenTest.IsNegativeTest" />
			<class name="MavenTest.IsPositiveTest" />
			<class name="MavenTest.MultiplyDoubleTest" />
			<class name="MavenTest.MultiplyTest" />
			<class name="MavenTest.PowTest" />
			<class name="MavenTest.SinTest" />
			<class name="MavenTest.SubDoubleTest" />
			<class name="MavenTest.SubLongTest" />
			<class name="MavenTest.SumDoubleTest" />
			<class name="MavenTest.SumLongTest" />
			<class name="MavenTest.TgTest" />
			<class name="MavenTest.TgTestException" />
		</classes>
	</test>
</suite> 
